{
	"name": "noresources/http", 
	"description": "HTTP utilities", 
	"type": "library", 
	"license": "MIT", 
	"keywords": [
		"http", 
		"psr-7"
	], 
	"authors": [
		{
			"name": "Renaud Guillard", 
			"email": "dev@nore.fr", 
			"role": "lead"
		}
	], 
	"autoload": {
		"psr-4": {
			"NoreSources\\Http\\": "src/", 
			"NoreSources\\Http\\Test\\": "tests/Shared"
		}
	}, 
	"autoload-dev": {
		"psr-4": {
			"NoreSources\\Http\\Test\\": "tests/Private", 
			"NoreSources\\Http\\Tools\\": "tools/src"
		}
	}, 
	"require": {
		"php": ">= 7.1", 
		"noresources/data": "^3.0",
		"noresources/languagetag": "^2.0",
		"psr/http-message": "^1.0 || ^2.0", 
		"psr/http-server-middleware": "^1.0"
	}, 
	"scripts": {
		"test": "@php  ./vendor/bin/phpunit --color=never --bootstrap vendor/autoload.php tests"
	}, 
	"require-dev": {
		"phpunit/phpunit": ">= 5.0", 
		"laminas/laminas-diactoros": "^2.3 || ^3.0", 
		"laminas/laminas-httphandlerrunner": "^1.2 || ^2.0", 
		"nette/php-generator": "^3.4"
	}, 
	"extra": {
		"branch-alias": {
			"dev-1.0.x": "1.0.x-dev", 
			"dev-1.1.x": "1.1.x-dev", 
			"dev-1.2.x": "1.2.x-dev", 
			"dev-1.3.x": "1.3.x-dev", 
			"dev-2.0.x": "2.0.x-dev", 
			"dev-2.1.x": "2.1.x-dev", 
			"dev-2.2.x": "2.2.x-dev",
			"dev-main": "3.0.x-dev"
		}
	}, 
	"replace": {
		"noresources/ns-php-http": "3.0"
	}, 
	"support": {
		"issues": "https://github.com/noresources/php-http/issues"
	}
}
